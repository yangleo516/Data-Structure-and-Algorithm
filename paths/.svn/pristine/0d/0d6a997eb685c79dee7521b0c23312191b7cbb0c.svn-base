
public class Node<T> {
	T value;
	int key;
	Handle handle = new Handle();
	
	public Node(int key, T value) {
		this.key = key;
		this.value = value;
	}
	
	public void setKey(int key) {
		this.key = key;
	}
	
	public int getKey() {
		return this.key;
	}
	
	public T getValue() {
		return this.value;
	}
	
	public Handle getHandle() {
		return this.handle;
	}
}
